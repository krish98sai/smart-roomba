/*
  Credentials.cpp - Change IP_ADDRESS_PLACEHOLDER, SSID_PLACEHOLDER,
                    and PASSWORD_PLACEHOLDER to your desired local
                    ip address, ssid, and password, respectively.
*/
#ifndef Credentials_h
#define Credentials_h
#define IP_ADDRESS "IP_ADDRESS_PLACEHOLDER (ex. 10.0.0.60)"
#define SSID  "SSID_PLACEHOLDER"
#define PASSWORD  "PASSWORD_PLACEHOLDER"
#endif
